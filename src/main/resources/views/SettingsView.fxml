<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="231.0" prefWidth="430.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.frm.tradecalculatormaven.SettingsViewController">
   <children>
      <Label layoutX="85.0" layoutY="71.0" text="Risk" />
      <Label layoutX="52.0" layoutY="116.0" text="OCO Limit" />
      <TextField fx:id="riskTxt" layoutX="123.0" layoutY="67.0" onKeyPressed="#riskEventHandler" prefHeight="25.0" prefWidth="76.0" />
      <TextField fx:id="ocoTxt" layoutX="123.0" layoutY="112.0" onKeyPressed="#ocoLimitEventHandler" prefHeight="25.0" prefWidth="76.0" />
      <Button fx:id="saveBtn" layoutX="96.0" layoutY="171.0" mnemonicParsing="false" onAction="#saveBtnHandler" text="Save" />
      <Button fx:id="exitBtn" layoutX="283.0" layoutY="171.0" mnemonicParsing="false" onAction="#exitBtnHandler" text="Exit" />
   </children>
</AnchorPane>
